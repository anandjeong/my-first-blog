		var keyCodeVal = function(e){
			var event = e || window.event; 
			var keycode = event.keyCode || e.which; 
			if (keycode == 32 ) { 
				alert("°ø¹éÀº Çã¿ëºÒ°¡");
				$('#m_id').val('');
