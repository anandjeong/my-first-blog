		var DealView = function(idx){
			location.href="newcar_mall_deal.asp?idx="+idx;
		}
		var viewMore = function(){
			var viewCnt = Number($('#viewCnt').val());
			viewCnt = viewCnt + 5;
			$('#viewCnt').val(viewCnt);
			for (var i = 0; i <= viewCnt-1; i++){
				$('#viewDiv_'+i).show();
			}
		}

		var searchBtn = function(){
			var sido = $('#addr1').val();
			var stat = $('#regiStatus').val();
			location.href="mall_list_newcar.asp?search1="+sido+"&search2="+stat;
		}
	</script>
